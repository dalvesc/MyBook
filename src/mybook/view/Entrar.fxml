<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="360.0" prefWidth="528.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mybook.controller.EntrarController">
    <children>
        <Button fx:id="cadastrarButton" layoutX="384.0" layoutY="246.0" onAction="#cadastrarButtonAction" text="Cadastre-se">
         <font>
            <Font size="14.0" />
         </font></Button>
      <TextField fx:id="email" layoutX="49.0" layoutY="62.0" onAction="#getEmail" promptText="Email" />
      <Label layoutX="49.0" layoutY="180.0" text="MyBook" textFill="#18cd00">
         <font>
            <Font size="45.0" />
         </font>
      </Label>
      <TextField fx:id="password" layoutX="310.0" layoutY="62.0" onAction="#getPassword" promptText="Password" />
      <Button fx:id="entrarButton" layoutX="407.0" layoutY="108.0" mnemonicParsing="false" onAction="#entrarButtonAction" text="Entrar" />
    </children>
</AnchorPane>
